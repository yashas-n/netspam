<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>UserRegister</servlet-name>
        <servlet-class>UserRegister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>A_AddCategory</servlet-name>
        <servlet-class>A_AddCategory</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>A_AddPosts</servlet-name>
        <servlet-class>A_AddPosts</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserRegister</servlet-name>
        <url-pattern>/UserRegister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>A_AddCategory</servlet-name>
        <url-pattern>/A_AddCategory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>A_AddPosts</servlet-name>
        <url-pattern>/A_AddPosts</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
